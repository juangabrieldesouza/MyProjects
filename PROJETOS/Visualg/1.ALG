algoritmo "Primeiro"
var
   kkk: inteiro
   msg: caractere
   copo: caractere
inicio
      kkk <- (-2)
      msg <- ("24000")
      copo <- ("Ola marinheiros! hoje temos uma importante missão no alto mar")
      Escreval (copo)
    Escreval("Jesus salva, cura e liberta!")
    Escreval(kkk)
    Escreval(msg)
    Escreva("Isso aew, conseguimos")
fimalgoritmo